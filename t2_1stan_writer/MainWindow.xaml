                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="40,167,0,0" Name="comboBox8" VerticalAlignment="Top" Width="284" Grid.Column="1" DisplayMemberPath="Value" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="40,205,0,0" Name="comboBox9" VerticalAlignment="Top" Width="284" Grid.Column="1" DisplayMemberPath="Value" />
