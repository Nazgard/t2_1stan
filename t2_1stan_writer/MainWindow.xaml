                    <Label Content="Время смены" Height="28" Margin="40,86,0,0" x:Name="label2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="84" />
                    <Label Content="Специалист АСК ТЭСЦ 2" Height="28" Margin="40,209,0,0" x:Name="label3" VerticalAlignment="Top" HorizontalAlignment="Left" Width="143" />
                    <Label Content="Специалист ОККП" Height="28" Margin="40,257,0,0" x:Name="label4" VerticalAlignment="Top" HorizontalAlignment="Left" Width="109" />
                </Grid>
            </TabItem>
            <TabItem Header="Параметры плавки" Name="tabItem2" IsEnabled="False">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="23*"/>
                        <RowDefinition Height="4*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="360*" />
