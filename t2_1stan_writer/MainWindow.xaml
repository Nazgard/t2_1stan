                    <Button Content="Далее" Margin="0,13,10,10" Name="button7" Grid.Column="1" Click="button2_Click" HorizontalAlignment="Right" Width="100" Grid.Row="1" />
                    <ComboBox Height="23" Margin="6,43,0,0" Name="comboBox1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="284" DisplayMemberPath="Value" Grid.Column="1" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="6,91,0,0" Name="comboBox2" VerticalAlignment="Top" Width="284" DisplayMemberPath="Value" Grid.Column="1" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="6,214,0,0" Name="comboBox3" VerticalAlignment="Top" Width="284" DisplayMemberPath="Value" Grid.Column="1" />
                    <ComboBox Height="23" HorizontalAlignment="Left" Margin="6,262,0,0" Name="comboBox4" VerticalAlignment="Top" Width="284" DisplayMemberPath="Value" Grid.Column="1" />
