                    <ComboBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="40,243,0,0" Name="comboBox11" VerticalAlignment="Top" Width="284" DisplayMemberPath="Value" />
                    <Label Content="Установка" Height="28" HorizontalAlignment="Left" Margin="40,238,0,0" Name="label10" VerticalAlignment="Top" />
                    <Label Content="Порог" Height="28" HorizontalAlignment="Left" Margin="40,276,0,0" Name="label11" VerticalAlignment="Top" />
                    <Label Content="Ток" Height="28" HorizontalAlignment="Left" Margin="40,309,0,0" Name="label12" VerticalAlignment="Top" />
                    <Label Content="d" Grid.Column="1" Height="28" HorizontalAlignment="Left" Margin="18,129,0,0" Name="label13" VerticalAlignment="Top" />
                    <Label Content="Но" Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="221,129,0,0" Name="label14" VerticalAlignment="Top" />
                    <TextBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="247,129,0,0" Name="textBox1" VerticalAlignment="Top" Width="77" PreviewTextInput="textBox1_PreviewTextInput" />
                    <TextBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="40,281,0,0" Name="textBox2" VerticalAlignment="Top" Width="284" Text="50" PreviewTextInput="textBox2_PreviewTextInput" />
                    <TextBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="40,314,0,0" Name="textBox3" VerticalAlignment="Top" Width="284" Text="1" PreviewTextInput="textBox3_PreviewTextInput" />
                    <TextBox Grid.Column="1" Height="23" HorizontalAlignment="Left" Margin="40,48,0,0" Name="textBox4" VerticalAlignment="Top" Width="284" Text="1" PreviewTextInput="textBox4_PreviewTextInput" />
                    <Button Content="Далее" Grid.Column="1" Margin="0,10,10,10" Click="button3_Click" HorizontalAlignment="Right" Width="100" Grid.Row="1" />
                </Grid>
            </TabItem>
            <TabItem Header="Контроль" Name="tabItem3" IsEnabled="False">
                <Grid>
                    <Canvas x:Name="Canvas" Background="Black" IsEnabled="False">
                        <Rectangle Canvas.Left="50" Canvas.Top="192" Height="100" Name="tube" Stroke="Green" Width="0" Fill="Green" />
                    </Canvas>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
